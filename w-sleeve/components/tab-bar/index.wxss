
.sleeve {
    font-family: w-sleeve !important;
    color:#157658 !important;
}

.container {
    display: flex;
    flex-direction: row;
    background-color: white;
    width: 100%;
    height: 100rpx;
}

.icons {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-around;
    width: 230rpx;
    height: 100%;
    color: white;
    padding-left: 16rpx;
    padding-right: 22rpx;
    box-sizing: border-box;
    color: #999999;
}

.icons-home {
    width: 50%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.icons-cart {
    width: 50%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding-top: 6rpx;
}

.icons-text {
    font-size: 20rpx;
}

.btn-font {
    color: white;
    font-size: 32rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.cart {
    background-color: #333333;
    width: 260rpx;
    height: 100%;
}

.buy {
    background-color: #157658;
    width: 260rpx;
    height: 100%;
}